{
  "homepage": "./",
  "name": "exokit-launcher",
  "description": "Exokit Launcher | Settings, Updates and Current News.",
  "main": "public/electron.js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "concurrently": "^4.0.1",
    "electron": "^3.0.5",
    "electron-is-dev": "^1.0.1",
    "eslint": "^5.7.0",
    "jquery": "^3.3.1",
    "pkg": "^4.3.4",
    "popper.js": "^1.14.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5",
    "wait-on": "^3.1.0"
  },
  "devDependencies": {
    "electron-packager": "^12.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build --em.main=build/electron.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.exokitlauncher.electron-cra",
    "files": [
      "build/",
      "node_modules/"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
